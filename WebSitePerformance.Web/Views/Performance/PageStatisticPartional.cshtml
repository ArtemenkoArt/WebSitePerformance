@model WebSitePerformance.Core.Models.PageStatistic


    <div class="row">
        <div class="col-xs-4">
            <div>
                <strong>
                    @Html.DisplayNameFor(model => model.PageUrl)
                </strong>
                @Html.DisplayFor(model => model.PageUrl)
            </div>
        </div>

        <div class="col-xs-2">
            <strong>
                @Html.DisplayNameFor(model => model.Response)
            </strong>
