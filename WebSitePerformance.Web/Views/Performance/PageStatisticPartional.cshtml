            <strong>
                @Html.DisplayNameFor(model => model.ResponseMin)
            </strong>
            @Html.DisplayFor(model => model.ResponseMin)
        </div>

        <div class="col-xs-2">
            <strong>
                @Html.DisplayNameFor(model => model.ResponseQty)
